<div id="feedback-overview">
    <div class="feedback-logo">Feedback</div>
    <div id="feedback-overview-description">
        <div id="feedback-overview-description-text">
            <h3>Details</h3>
            <textarea class="form-control" rows="20"
                id="feedback-overview-note"
                placeholder="Please describe what you were trying to do when you encountered a problem and what happened, or give a suggestion."
                data-toggle="popover" data-trigger="hover" data-placement="left"
                title="Problem or suggestion" data-content="."
                ></textarea>
        </div>
    </div>
    <div id="feedback-overview-screenshot">
        <h3>Screenshot</h3>
    </div>
    <div class="feedback-buttons">
        <button id="feedback-overview-back" class="btn">Back</button>
        <button id="feedback-submit" class="btn btn-success btn-raised">Send</button>
    </div>
    <div id="feedback-overview-error">Please describe the problem or give a suggestion.</div>
    <div class="feedback-wizard-close"></div>
</duv>
